{
  "name": "backend",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "start": "bun src/index.ts",
    "build": "bun build src/index.ts --outdir ./dist --target bun",
    "db:generate": "bunx prisma generate",
    "db:migrate": "bunx prisma migrate dev",
    "db:migrate:deploy": "bunx prisma migrate deploy",
    "db:push": "bunx prisma db push",
    "db:studio": "bunx prisma studio",
    "auth:generate": "bunx @better-auth/cli generate",
    "setup": "bun install && bunx prisma generate && bunx prisma migrate dev"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/sharp": "^0.32.0",
    "prisma": "^6.17.1"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.918.0",
    "@aws-sdk/s3-request-presigner": "^3.918.0",
    "@prisma/adapter-pg": "^6.17.1",
    "@prisma/client": "^6.17.1",
    "@types/multer": "^2.0.0",
    "better-auth": "^1.3.28",
    "cors": "^2.8.5",
    "dodopayments": "^2.4.4",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "livekit-server-sdk": "^2.14.0",
    "multer": "^2.0.2",
    "resend": "^6.2.1",
    "sharp": "^0.34.4",
    "standardwebhooks": "^1.0.0",
    "zod": "^4.1.12"
  }
}